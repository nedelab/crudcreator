[
    {
        "name": "SimpleSQLAdaptator",
        "params": {
            "engine_wrapper": "$engine_wrapper$"
        },
        "interface": {
            "name": "table21",
            "can_indicate_read_distinct": true,
            "fields": [
                {
                    "name": "column_1"
                },
                {
                    "name": "column_2",
                    "nullable": false
                }
            ]
        }
    },
    {
        "name": "Rename",
        "params": {
            "interface_name": "entity21",
            "translation_list": [
                {
                    "source": "column_1",
                    "destination": "field_1"
                },
                {
                    "source": "column_2",
                    "destination": "field_2"
                }
            ]
        }
    },
    {
        "name": "ReadFirewall",
        "params": {
            "list_readable_field_name": [
                "field_1", 
                "field_2"
            ]
        }
    },
    {
        "name": "CreateFirewall",
        "params": {
            "list_creatable_field_name": [
                "field_1", 
                "field_2"
            ]
        }
    },
    {
        "name": "UpdateFirewall",
        "params": {
            "list_updatable_field_name": [
                "field_1", 
                "field_2"
            ]
        }
    },
    {
        "name": "IdFirewall",
        "params": {
            "list_id_field_name": [
                "field_2"
            ]
        }
    },
    {
        "name": "FilterFirewall",
        "params": {
            "allowed_filter_on_fields": [
                {
                    "field_name": "field_2",
                    "allowed_filter_type": {
                        "filtration_type": "equal", 
                        "is_mandatory": false,
                        "default": null
                    }
                }
            ]
        }
    },
    {
        "name": "AddDefault",
        "params": {
            "default_on_fields": [
                {
                    "field_name": "field_2",
                    "default": 10
                },
                {
                    "field_name": "field_1",
                    "default": "$random_int$"
                }
            ]
        }
    }
]