    {
        "name": "IdFirewall",
        "params": {
            "list_id_field_name": [
                "field_to_join"
            ]
        }
    },
    {
        "name": "FilterFirewall",
        "params": {
            "allowed_filter_on_fields": [
                {
                    "field_name": "field_to_join",
                    "allowed_filter_type": {
                        "filtration_type": "equal", 
                        "is_mandatory": false,
                        "default": null
                    }
                }
            ]
        }
    }
